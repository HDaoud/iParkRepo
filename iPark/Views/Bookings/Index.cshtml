        <td>
            @Html.DisplayFor(modelItem => item.booking_time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.checkin_time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.release_time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.checkout_time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParkingLot.status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.book_id.Trim() }) |
            @Html.ActionLink("Details", "Details", new { id = item.book_id.Trim() }) |
