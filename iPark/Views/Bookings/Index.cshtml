@model IEnumerable<iPark.Models.Booking>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.booking_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.checkin_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.release_time)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.checkout_time)
        </th>
        <th>
